# bruteforcing hashed known_hosts
oclHashcat -m 160 -a 1 hash.txt ips-left.txt ips-right.txt --hex-salt
hashcat -a 3 -m 160 hash.txt ipv4.hcmask --hex-salt # wget 'http://pastebin.com/raw.php?i=4HQ6C8gG' -O ipv4.hcmask
